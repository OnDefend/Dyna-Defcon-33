{
  "ml_enhanced_confidence": {
    "enabled": true,
    "models_dir": "models/confidence",
    "data_dir": "data/confidence",
    "min_evidence_threshold": 0.3,
    "uncertainty_weight": 0.2,
    "ensemble_weight": 0.3,
    "context_weight": 0.2,
    "calibration_window_days": 7,
    "max_calibration_history": 1000,
    "confidence_factors": {
      "evidence_strength": {
        "enabled": true,
        "weight": 0.2,
        "min_sources": 2,
        "pattern_match_bonus": 0.1
      },
      "pattern_reliability": {
        "enabled": true,
        "weight": 0.2,
        "min_calibration_samples": 5,
        "framework_penalty": 0.8
      },
      "context_support": {
        "enabled": true,
        "weight": 0.2,
        "file_type_bonus": 0.15,
        "test_code_penalty": -0.1
      },
      "ensemble_agreement": {
        "enabled": true,
        "weight": 0.3,
        "min_models": 3,
        "variance_scaling": 10
      },
      "uncertainty_quantification": {
        "enabled": true,
        "weight": 0.2,
        "base_confidence_factor": 0.3,
        "evidence_factor": 0.2,
        "novelty_factor": 0.2
      }
    },
    "calibration": {
      "enabled": true,
      "auto_record": true,
      "confidence_buckets": [
        0.3,
        0.5,
        0.7,
        0.9
      ],
      "min_bucket_size": 10
    },
    "explanation": {
      "enabled": true,
      "detail_level": "moderate",
      "include_factors": true,
      "include_uncertainty": true
    }
  },
  "confidence_integration": {
    "optimized_framework_filter": true,
    "ml_vulnerability_classifier": true,
    "enhanced_scan_status_reporter": true,
    "false_positive_learner": true,
    "auto_enhance_findings": true,
    "record_outcomes": true
  }
}